<!-- Version 0.0.1 -->
<editors>

  <editor id="BOOL">
    <range uom="2" subset="0,1" />
  </editor>

  <editor id="PERCENT">
    <!-- 51 is percentage -->
    <range uom="51" subset="0-100" />
  </editor>

  <editor id="POWER">
    <!-- 78 is Power 0 = Off, 100 = On -->
    <range uom="78" subset="0,100" />
  </editor>

  <editor id="FANSPEED">
    <range uom="25" subset="0-4" nls="FANSPEED" />
  </editor>

  <editor id="PICO">
    <range uom="25" subset="1-5" nls="PICO" />
  </editor>

  <editor id="DEVTYPE">
    <range uom="25" subset="1-30" nls="DEVTYPE" />
  </editor>

  <editor id="ROOMSTATUS">
    <range uom="25" subset="0-2" nls="ROOMSTATUS" />
  </editor>

  <editor id="PHANTOM">
    <range uom="0" subset="1-100"/>
  </editor>

</editors>
